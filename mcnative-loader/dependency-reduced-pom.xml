        </exclusion>
        <exclusion>
          <artifactId>prematicdatabasequery-api</artifactId>
          <groupId>net.prematic.databasequery</groupId>
        </exclusion>
        <exclusion>
          <artifactId>prematicdatabasequery-sql</artifactId>
          <groupId>net.prematic.databasequery</groupId>
        </exclusion>
        <exclusion>
          <artifactId>HikariCP</artifactId>
          <groupId>com.zaxxer</groupId>
        </exclusion>
        <exclusion>
          <artifactId>h2</artifactId>
          <groupId>com.h2database</groupId>
        </exclusion>
        <exclusion>
          <artifactId>mysql-connector-java</artifactId>
          <groupId>mysql</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>net.prematic.libraries</groupId>
      <artifactId>prematiclibraries-plugin</artifactId>
      <version>1.0.31-SNAPSHOT</version>
      <scope>provided</scope>
      <exclusions>
        <exclusion>
          <artifactId>prematiclibraries-document</artifactId>
          <groupId>net.prematic.libraries</groupId>
        </exclusion>
        <exclusion>
          <artifactId>prematiclibraries-utility</artifactId>
