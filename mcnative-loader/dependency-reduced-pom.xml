<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>McNative</artifactId>
    <groupId>org.mcnative</groupId>
    <version>1.0.60-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>mcnative-loader</artifactId>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.1</version>
        <configuration>
          <source>8</source>
          <target>8</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-source-plugin</artifactId>
        <version>3.1.0</version>
        <executions>
          <execution>
            <id>attach-sources</id>
            <goals>
              <goal>jar</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>${plugin.shade.version}</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>spigot-repo</id>
      <url>https://hub.spigotmc.org/nexus/content/repositories/snapshots/</url>
    </repository>
    <repository>
      <id>bungeecord-repo</id>
      <url>https://oss.sonatype.org/content/repositories/snapshots</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>org.mcnative</groupId>
      <artifactId>mcnative-common</artifactId>
      <version>1.0.60-SNAPSHOT</version>
      <scope>provided</scope>
      <exclusions>
        <exclusion>
          <artifactId>prematiclibraries-utility</artifactId>
          <groupId>net.prematic.libraries</groupId>
        </exclusion>
        <exclusion>
          <artifactId>prematiclibraries-concurrent</artifactId>
          <groupId>net.prematic.libraries</groupId>
        </exclusion>
        <exclusion>
          <artifactId>prematiclibraries-command</artifactId>
          <groupId>net.prematic.libraries</groupId>
        </exclusion>
        <exclusion>
          <artifactId>prematiclibraries-event</artifactId>
          <groupId>net.prematic.libraries</groupId>
        </exclusion>
        <exclusion>
          <artifactId>prematiclibraries-dependency</artifactId>
          <groupId>net.prematic.libraries</groupId>
        </exclusion>
        <exclusion>
          <artifactId>prematiclibraries-caching</artifactId>
          <groupId>net.prematic.libraries</groupId>
        </exclusion>
        <exclusion>
          <artifactId>prematiclibraries-message</artifactId>
